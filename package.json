{
  "name": "aware-challenge-task",
  "version": "1.0.0",
  "description": "aappointment booking service",
  "main": "index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "dev": "tsx --watch src/server.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:seed": "tsx prisma/seed.ts",
    "lint": "npx @biomejs/biome check ./src",
    "lint:fix": "npx @biomejs/biome format --write ./src",
    "test": "npx prisma migrate reset --force && npm run prisma:seed && jest",
    "test:watch": "jest --watch",
    "test:cov": "npx prisma migrate reset --force && npm run prisma:seed && jest --coverage"
  },
  "keywords": [
    "aware",
    "challenge",
    "appointment",
    "booking",
    "service"
  ],
  "author": "shyam",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.7.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "reflect-metadata": "^0.2.2",
    "uuid": "^11.1.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@faker-js/faker": "^9.7.0",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.3",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "prisma": "^6.7.0",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.15",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
